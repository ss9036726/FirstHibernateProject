<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.UserAdd" table="alluser">
        <id name="id" type="java.lang.Integer"></id>
        <property name="username" column="username"></property>
        <property name="password" column="password"></property>
        <property name="studentid" column="studentid"></property>
        <property name="email" column="email"></property>
        <property name="address" column="address"></property>
        <property name="mobile" column="mobile"></property>
        <property name="pincode" column="pincode"></property>
    </class>
</hibernate-mapping>

